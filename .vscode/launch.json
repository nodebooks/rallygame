{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "program": "${workspaceRoot}/noderally_proto2/server/server.js"
        },
        {
            "type": "node",
            "request": "attach",
            "name": "Attach to child process",
            "processId": "${command:PickProcess}",
            "port": 5858
        },
        {
            "name": "Docker: Attach to Node",
            "type": "node",
            "request": "attach",
            "port": 9229,
            "address": "localhost",
            "localRoot": "${workspaceRoot}",
            "remoteRoot": "/usr/src/app"
        }
    ]
}